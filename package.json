{
  "name": "vincelivemix",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node --async-stack-traces dist/main.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint:fix": "npm rum lint -- --fix",
    "prettier": "prettier -l '{src,test}/**/*.{ts,yml,yaml,json,md}'",
    "prettier:write": "prettier --write '{src,test}/**/*.{ts,yml,yaml,json,md}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:dev": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config/typeorm-cli.config.ts",
    "typeorm:prod": "./node_modules/typeorm/cli.js --config dist/config/typeorm-cli.config.js",
    "migration:generate": "npm run typeorm:dev migration:generate -- -n",
    "migration:create": "npm run typeorm:dev migration:create -- -n",
    "migration:run": "npm run typeorm:dev migration:run",
    "migration:run:prod": "npm run typeorm:prod migration:run"
  },
  "dependencies": {
    "@nestjs/common": "7.0.11",
    "@nestjs/core": "7.0.11",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "7.0.11",
    "@nestjs/typeorm": "7.0.0",
    "bcrypt": "^4.0.1",
    "class-transformer": "0.2.3",
    "class-validator": "0.12.2",
    "dotenv": "8.2.0",
    "feed": "4.1.0",
    "multer": "1.4.2",
    "nest-winston": "1.3.4",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "8.1.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.5.5",
    "typeorm": "0.2.24",
    "uuid": "8.0.0",
    "winston": "3.2.1",
    "xmlbuilder2": "2.1.2"
  },
  "devDependencies": {
    "@nestjs/testing": "7.0.11",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "4.17.6",
    "@typescript-eslint/eslint-plugin": "2.32.0",
    "@typescript-eslint/parser": "2.32.0",
    "@types/jest": "26.0.0",
    "@types/multer": "1.4.3",
    "@types/node": "14.0.1",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "2.0.9",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint": "7.0.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.20.2",
    "jest": "26.0.1",
    "prettier": "2.0.5",
    "supertest": "4.0.2",
    "ts-jest": "25.5.1",
    "ts-node": "8.10.1",
    "tsc-watch": "4.2.3",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.2",
    "typescript": "3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
